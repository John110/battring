<h1>Listing create_matchings</h1>
<%= link_to 'New Create matching', new_project_matching_path(@project.id) %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Owner</th>
      <th>Title</th>
      <th>Reguration</th>
      <th>Limit</th>
      <th>Start</th>
      <th>Memo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_matchings.each do |create_matching| %>
      <tr>
        <td><%= link_to "#{create_matching.name}", project_matching_path(create_matching.project_id, create_matching.id), data: { "no-turbolink" => true } %></td>
        <td><%= create_matching.owner %></td>
        <td><%= create_matching.title %></td>
        <td><%= create_matching.reguration %></td>
        <td><%= create_matching.limit %></td>
        <td><%= create_matching.start %></td>
        <td><%= create_matching.memo %></td>
        <td><%= link_to 'Edit', edit_project_matching_path(create_matching.project_id, create_matching.id) %></td>
        <td><%= link_to 'Destroy', project_matching_path(create_matching.project_id, create_matching.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

